rom \"react-router-dom\"\n\n\nexport default function Login() {\n\n  const [emailLog, setEmailLog] = useState('');\n  const [passwordLog, setPasswordLog] = useState('');\n\n  const [message, setMessage] = useState('');\n\n  let history = useHistory();\n  Axios.defaults.withCredentials = true;\n\n  const login = () => {\n    Axios.post('http://localhost:5000/api/auth/signin', {\n      data: {\n          email: emailLog,\n          mdp: passwordLog,\n      }\n      }).then(res => {\n          console.log(res)\n          localStorage.setItem('token', res.data.token)\n          localStorage.setItem('email', res.data.user.email)\n          localStorage.setItem('prenom', res.data.user.prenom)\n          localStorage.setItem('nom', res.data.user.nom)\n          localStorage.setItem('bio', res.data.user.bio)\n          localStorage.setItem('adresse', res.data.user.adresse)\n          // localStorage.setItem('telephone', res.data.user.telephone)\n          history.push(\"/\");\n      }).catch(err => {\n          console.log(err.response.data);\n          setMessage(err.response.data);\n      })\n}\n\n// useEffect(() => {\n//   Axios.get('http://localhost:5000/auth/signin').then((res) => {\n//     console.log(res);\n//   })\n// }, [])\n\n  return (\n    <div className=\"login\">\n      <div className=\"loginWrapper\">\n        <div className=\"loginLeft\">\n        <h3 className=\"loginLogo\">üçë Fessebook</h3>\n            <span className=\"loginDesc\">\n                Partagez vos fesses a tout le monde ! üòè\n            </span>\n        </div>\n        <div className=\"loginRight\">\n          <form className=\"loginBox\">\n            <input\n              placeholder=\"E-mail\"\n              type=\"email\"\n              required\n              className=\"loginInput\"\n              onChange={(e) => {setEmailLog(e.target.value)}}\n            />\n            <input\n              placeholder=\"Mot de passe\"\n              type=\"password\"\n              required\n              minLength=\"6\"\n              className=\"loginInput\"\n              onChange={(e) => {setPasswordLog(e.target.value)}}\n            />\n            {message}\n            <button className=\"loginButton\" type=\"submit\" onClick={login}>Se connecter</button>\n            <span className=\"loginForgot\">Mot de passe oubli√© ?</span>\n            <Link to=\"/register\" className=\"loginRegisterButton\">Cr√©er mon compte</Link>\n          </form>\n        </div>\n      </div>\n    </div>\n  );\n}\n","/Users/owen/Documents/Fessebook/client/src/pages/profile/Profile.jsx",[],"/Users/owen/Documents/Fessebook/client/src/components/topbar/Topbar.jsx",[],"/Users/owen/Documents/Fessebook/client/src/components/sidebar/Sidebar.jsx",[],"/Users/owen/Documents/Fessebook/client/src/components/feed/Feed.jsx",[],"/Users/owen/Documents/Fessebook/client/src/components/rightbar/Rightbar.jsx",[],"/Users/owen/Documents/Fessebook/client/src/dummyData.js",[],"/Users/owen/Documents/Fessebook/client/src/components/friends/Friends.jsx",[],"/Users/owen/Documents/Fessebook/client/src/components/share/Share.jsx",[],"/Users/owen/Documents/Fessebook/client/src/components/post/Post.jsx",[],"/Users/owen/Documents/Fessebook/client/src/components/online/Online.jsx",[],{"ruleId":"66","severity":1,"message":"67","line":8,"column":15,"nodeType":"68","messageId":"69","endLine":8,"endColumn":25},{"ruleId":"66","severity":1,"message":"70","line":2,"column":19,"nodeType":"68","messageId":"69","endLine":2,"endColumn":28},"no-unused-vars","'useHistory' is defined but never used.","Identifier","unusedVar","'useEffect' is defined but never used."]